{
  "description": "A study to assess how individuals interact with their search engines.",
  "author": "Kartikeya Kandula",
  "manifest_version": 2,
  "name": "Search Engine Usage Study",
  "version": "0.4.0",
  "homepage_url": "https://github.com/mozilla-ion/ion-basic-study",
  "applications": {
    "gecko": {
      "id": "ion-basic-study@mozilla.org",
      "strict_min_version": "77.0a1"
    }
  },
  "permissions": [
    "<all_urls>",
    "idle",
    "notifications",
    "storage",
    "unlimitedStorage",
    "tabs",
    "webRequest",
    "webNavigation",
    "history"
  ],
  "experiment_apis": {
    "experimental": {
      "schema": "./experimental/schema.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "script": "./experimental/api.js",
        "paths": [
          [
            "experimental"
          ]
        ]
      }
    }
  },
  "background": {
    "scripts": [
      "dist/background.js"
    ]
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "icons/princeton_university_shield_book_gray.svg"
    }
  }
}